<!DOCTYPE html>
<!--[if lt IE 7]><html class="ie9 ie8 ie7 ie6" lang="en"><![endif]-->
<!--[if IE 7]><html class="ie9 ie8 ie7" lang="en"><![endif]-->
<!--[if IE 8]><html class="ie9 ie8" lang="en"><![endif]-->
<!--[if IE 9]><html class="ie9" lang="en"><![endif]-->
<!--[if gt IE 9]><!--><html lang="en"><!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  
  <title>{{ name }} &ndash; {{ short_description }}</title>
  
  <!-- Favicon -->
  <link rel="icon" type="image/png" sizes="32x32" href="/docs/img/favicon.png">
    
  <!-- Stylesheets -->
  <link rel="stylesheet" type="text/css" media="all" href="/docs/css/baseweb.min.css">
  
  <!-- Typekit -->
  <script src="//use.typekit.net/ult4xng.js"></script>
  <script>try{Typekit.load();}catch(e){}</script>
  
  <!--[if lt IE 9]>
  <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
  
</head>
<body>
  
<div id="wrapper" class="wrapper scene">
  
  <header class="header">
    <div class="inner-header">
    
      <h1><a href="/" class="logo">{{ name }}</a></h1>
      
      <aside class="sidebar">
        <nav class="nav">
          {{ > nav }}
        </nav>
      </aside><!-- .sidebar -->
    
    </div>
    
    <div class="action">
      <small class="version">Currently v{{ version }}</small>
      <a href="{{ repository.url }}" class="button primary">Download {{ name }}</a>
    </div>
        
  </header><!-- .header -->
  
  <section class="main">
  
    <div class="content">
      {{ > page }}
    </div><!-- .content -->
      
  </section><!-- .main -->
   
</div><!-- .wrapper -->

<!-- jQuery -->
<script src="http://code.jquery.com/jquery-2.1.3.min.js"></script>
<script>!window.jQuery && document.write(unescape('%3Cscript src="/docs/js/libs/jquery.min.js"%3E%3C/script%3E'))</script>

<!-- JavaScript -->
<script src="/docs/js/scripts.min.js"></script>

</body>
</html>