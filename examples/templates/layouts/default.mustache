<body>
  
<div id="wrapper" class="wrapper scene">
<div class="template template-example scene-element vertical-full">

  <header class="example-header">
    <h1><a href="/" class="logo">{{ name }}</a></h1>
    
    <div class="widget return-widget">
      {{#page.return}}
        <a href="{{ url }}" class="btn btn-light-on-black">&larr; Back</a>
      {{/page.return}}
      {{^page.return}}
        <a href="/docs/" class="btn btn-light-on-black">&larr; Back</a>
      {{/page.return}}
    </div>
    
    <div class="widget display-widget">
      <a href="{{ repository.url }}" class="btn btn-light-on-black">Download</a>
    </div>
    
  </header><!-- .example-header -->
  
  <div class="example-main">
    
    {{ > page }}
      
  </div><!-- .example-main -->

</div><!-- .scene-element -->
</div><!-- .scene -->

<!-- jQuery -->
<script src="http://code.jquery.com/jquery-2.1.3.min.js"></script>
