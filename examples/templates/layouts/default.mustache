<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  
  <title>{{ name }} &ndash; {{ short_description }}</title>
  
  <!-- Favicon -->
  <link rel="icon" type="image/png" sizes="32x32" href="/docs/img/favicon.png">
  
  <!-- Stylesheets -->
  <link rel="stylesheet" type="text/css" media="all" href="/docs/css/baseweb.min.css">
  
  <!-- Typekit -->
  <script src="//use.typekit.net/ult4xng.js"></script>
  <script>try{Typekit.load();}catch(e){}</script>
  
